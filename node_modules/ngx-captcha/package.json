    "Angular Captcha",
    "Angular reCaptcha",
    "reCaptcha v2",
    "reCaptcha v3",
    "Invisible reCaptcha",
    "google reCaptcha"
  ],
  "private": false,
  "peerDependencies": {
    "@angular/common": ">= 15.0.0",
    "@angular/core": ">= 15.0.0"
  },
  "module": "fesm2022/ngx-captcha.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
