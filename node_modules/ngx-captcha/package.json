{
  "name": "ngx-captcha",
  "version": "14.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Enngage/ngx-captcha"
  },
  "author": "richard sustek",
  "description": "Dynamic captcha (Google reCaptcha) implementation for Angular",
  "keywords": [
